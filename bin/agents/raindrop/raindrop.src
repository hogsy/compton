proc Tick
    call IsVisible
    if $r0 == 0
        $position
    endif
    return

proc Tick() {
    if(!IsVisible()) {
        Remove($self);
    }

    $position.y += 0.5;
    $position.x += GetWindSpeed($position) / 10.0;
    $angle = -(GetWindSpeed($position) / 100.0);
}
